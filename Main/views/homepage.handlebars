<!-- Loop over each object in the projects array -->


{{#each users as |user| }}
<div class="matches_container">
<div class="match">
    <div class="match_name">
        <h1>{{user.first_name}}</h1>
        <span class="pronoun">{{user.preferred_pronoun}}</span>
        <span class="age"><strong>{{user.age}}</strong></span>
<h3 class="beer_preference">{{user.beers_name}}</h3>
        <h4 class="hobbies">Hobbies: {{user.hobbies}}</h4>
        <ul class="hobbies_list">
            {{#each user.user_hobbies}}
            <li>
                {{{this}}}
            </li>
            {{/each}}
        </ul>
    </div>
</div>
</div>
{{/each}}

