<!-- Loop over each object in the projects array -->



<div class="matches_container">

        <div class="match row row-cols-1 row-cols-md-2">
            {{#each users as |user| }}
            <div class="match_card card hvr-grow">
                {{!-- <div class="container">
                    <div class="row"> --}}
                <div class="name_photo col-sm-6">
                    <h1 class="card-title">{{user.first_name}}</h1>
                    <img class="circle profilePhoto col-sm-6" src="" >
                    {{!-- </div>
                    </div> --}}
                </div><!--ending name and photo div-->
                <div class="cheers"><button class="cheers_btn">Cheers {{user.first_name}}</button></div>
                 <span class="age">{{user.age}}</span>
                 <span class="pronoun">{{user.preferred_pronoun}}</span>
                    <span class="beer_preference"><strong>Beer of Choice: </strong>{{user.beers_name}}</span>
                  
                    
                </div >
                {{/each}}<!--ending div match_name-->
        </div><!--ending div match card-->
     
</div><!---ending div for container-->



